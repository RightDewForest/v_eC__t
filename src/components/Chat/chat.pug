.view
  .channel
    ul
      li(v-for="(channel) in channels") 
        router-link(:to="{name: 'channel', params: { cname: channel }}") {{ channel }}
  .channelName {{ $route.params.cname }}
  .messages
    ul
      li(v-for="(mes) in messages")
        .message {{ mes.body }}
        .date {{ mes.date }}
        .name {{ mes.user.name }}
  .form
    input(type="text" v-model="message")
    input(type="button" value="SEN...D?" @click="")